<input class="form-control w-100 m-5" type="text" required name="id" hidden value="<%= post.id %>"/>
<input class="form-control w-60p m-5" type="text" required name="titulo" value="<%= post.titulo%>"/>
<input class="form-control w-30p m-5" type="text" required name="categoria" value="<%= post.categoria%>"/>
<input class="form-control w-90p m-5 " type="file" required name="image" value="<%= post.image %>"/>
<textarea class="form-control w-90p d-b m-5" required name="descricao" cols="30" rows="4"><%= post.descricao%></textarea>
